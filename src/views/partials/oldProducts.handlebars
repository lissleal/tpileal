
{{#each products}}
            <tr>
            <td>{{this.title}}</td>
            <td> {{this.id}} </td>
            <td> {{this.description}} </td>
            <td> {{this.price}} </td>
            <td> {{this.category}} </td>
            <td> {{this.stock}} </td>
            <td><img src="{{this.thumbnails}}" alt=""></td>
            </tr>
{{/each}}
